# Source MongoDB (исходная база данных с товарами - только чтение)
SOURCE_MONGO_HOST=mongodb.angara-ide.ts.net
SOURCE_MONGO_PORT=27017
SOURCE_MONGO_USER=
SOURCE_MONGO_PASS=
SOURCE_MONGO_AUTHSOURCE=
SOURCE_MONGO_AUTHMECHANISM=SCRAM-SHA-256
SOURCE_MONGO_DIRECT_CONNECTION=false
SOURCE_MONGODB_DATABASE=products
# Оставляем пустым - будем брать из всех коллекций
SOURCE_COLLECTION_NAME=

# Target MongoDB (наша новая база для классификации)
TARGET_MONGO_HOST=mongodb.angara-ide.ts.net
TARGET_MONGO_PORT=27017
TARGET_MONGO_USER=
TARGET_MONGO_PASS=
TARGET_MONGO_AUTHSOURCE=
TARGET_MONGO_AUTHMECHANISM=SCRAM-SHA-256
TARGET_MONGO_DIRECT_CONNECTION=false
TARGET_MONGODB_DATABASE=TenderDB
TARGET_COLLECTION_NAME=classified_products

# Redis
REDIS_URL=redis://localhost:6379

# Anthropic API
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022

# Prompt caching settings
ENABLE_PROMPT_CACHING=true
CACHE_TTL_MINUTES=5

# Proxy settings for Anthropic API (опционально)
HTTP_PROXY=
HTTPS_PROXY=
SOCKS_PROXY=

# Processing settings
MIGRATION_BATCH_SIZE=1000
CLASSIFICATION_BATCH_SIZE=250
MAX_WORKERS=1

# Rate limit settings
RATE_LIMIT_DELAY=6
MAX_RETRIES=3

# API Security
API_KEY=your_secure_api_key_here